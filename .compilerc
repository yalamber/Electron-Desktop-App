{
  "application/javascript": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ],
        "sourceMaps": "both"
      },
      "production": {
        "sourceMaps": "none"
      }
    }
  }
}
